{
  "name": "js-utilities",
  "version": "1.0.0",
  "description": "Various utilities for JavaScript ES6",
  "main": "utilities.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.1"
  },
  "scripts": {
    "document": "jsdoc",
    "test": "mocha --reporter min",
    "undo-last": "git reset --soft HEAD^",
    "push": "bash ./add-commit-push.sh -f",
    "push-skip-test": "npm run push -nt",
    "push-authed": "bash ./git-auth.sh && git push -f",
    "push-staged": "git push || npm run push-authed"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Oaphi/Utilities.git"
  },
  "keywords": [
    "utilities",
    "javascript",
    "es6"
  ],
  "author": "Oleg Valter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oaphi/Utilities/issues"
  },
  "mocha": {
    "watch": true,
    "fullTrace": false
  },
  "chai": {
    "config": {
      "showDiff": true
    }
  },
  "homepage": "https://github.com/Oaphi/Utilities#readme"
}

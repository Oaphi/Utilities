{
  "name": "@oaphi/utilities",
  "version": "0.5.1",
  "description": "Various utilities for JavaScript ES6",
  "main": "utilities.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@oaphi/distributor": "^1.3.2",
    "underscore": "^1.10.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.0.0",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "mocha": "^7.1.2"
  },
  "scripts": {
    "dist": "distribute -s",
    "dist-watch": "npm run dist -- -w",
    "document": "jsdoc -c configs/jsdoc.js",
    "test": "mocha --reporter min",
    "push": "bash ./add-commit-push.sh -f",
    "push-skip-test": "npm run push -nt"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Oaphi/Utilities.git"
  },
  "keywords": [
    "utilities",
    "javascript",
    "es6"
  ],
  "author": "Oleg Valter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Oaphi/Utilities/issues"
  },
  "mocha": {
    "watch": true,
    "fullTrace": false
  },
  "chai": {
    "config": {
      "showDiff": true
    }
  },
  "homepage": "https://github.com/Oaphi/Utilities#readme"
}
